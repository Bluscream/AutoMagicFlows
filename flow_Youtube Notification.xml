  <action type="notification_status_bar">
    <useDefaultName>true</useDefaultName>
    <name>Notification on Statusbar: Now playing Notification</name>
    <notificationIconType>CUSTOM</notificationIconType>
    <notificationIcon>NOTIFICATION</notificationIcon>
    <customNotificationIcon>app.icon://com.google.android.youtube</customNotificationIcon>
    <title>Now playing</title>
    <message>{global_youtube_current_title} by {global_youtube_current_author}</message>
    <sound>false</sound>
    <vibrate>false</vibrate>
    <flashLED>false</flashLED>
    <flashLEDColor>#ff00ff00</flashLEDColor>
    <flashLEDOn>500</flashLEDOn>
    <flashLEDOff>500</flashLEDOff>
    <flagLocalOnly>false</flagLocalOnly>
    <accentColorEnabled>false</accentColorEnabled>
    <accentColor>#ffff0000</accentColor>
    <flagOngoing>true</flagOngoing>
    <flagNoClear>false</flagNoClear>
    <notificationIDEnabled>false</notificationIDEnabled>
    <notificationID>999</notificationID>
    <category>TRANSPORT</category>
    <priority>MIN</priority>
    <visibility>PRIVATE</visibility>
    <messageBigEnabled>true</messageBigEnabled>
    <messageBig>{global_youtube_current_author}
&lt;b&gt;{global_youtube_current_title}&lt;/b&gt;</messageBig>
    <showProgressbar>false</showProgressbar>
    <progressbarIndeterminate>false</progressbarIndeterminate>
    <progressbarValue>0</progressbarValue>
    <progressbarMaximum>100</progressbarMaximum>
    <largeIconEnabled>false</largeIconEnabled>
    <largeIcon></largeIcon>
    <group></group>
    <isGroupSummary>false</isGroupSummary>
  </action>
  <action type="remove_notification_status_bar">
    <useDefaultName>true</useDefaultName>
    <name>Remove Notification on Statusbar: 999 (Automagic)</name>
    <automagicNotifications>true</automagicNotifications>
    <all>false</all>
    <notificationID>999</notificationID>
    <overall>true</overall>
    <packageName></packageName>
    <allOfApp>true</allOfApp>
    <filterNotificationID></filterNotificationID>
    <textMatchField>TEXT</textMatchField>
    <textMatchType>CONTAINS_TEXT</textMatchType>
    <textFilter></textFilter>
  </action>
  <flow type="flow">
    <name>Youtube Notification</name>
    <group>YouTube</group>
    <enabled>true</enabled>
    <executionPolicy>PARALLEL</executionPolicy>
    <triggercontainer id="t1" x="70.0" y="52.5">
      <trigger>Youtube stopped</trigger>
      <trigger>Youtube video changed</trigger>
    </triggercontainer>
    <conditioncontainer id="t2" x="-69.99999" y="297.5">Expression: trigger = "Youtube video changed"</conditioncontainer>
    <actioncontainer id="t3" x="-70.00005" y="437.5">Notification on Statusbar: Now playing Notification</actioncontainer>
    <conditioncontainer id="t4" x="174.99997" y="297.5">Expression: trigger = "Youtube stopped"</conditioncontainer>
    <actioncontainer id="t5" x="174.99998" y="437.5">Remove Notification on Statusbar: 999 (Automagic)</actioncontainer>
    <connection from="t1" to="t2" type="NORMAL" sourcePosition="SOUTH" targetPosition="NORTH" />
    <connection from="t1" to="t4" type="NORMAL" sourcePosition="SOUTH" targetPosition="NORTH" />
    <connection from="t2" to="t3" type="TRUE" sourcePosition="SOUTH" targetPosition="NORTH" />
    <connection from="t4" to="t5" type="TRUE" sourcePosition="SOUTH" targetPosition="NORTH" />
  </flow>
